{
    "name": "demo",
    "version": "4.0.0",
    "description": "Boilerplate for ReactJS project with ExpressJS server",
    "main": "server/bin/server.js",
    "scripts": {
        "dev": "webpack-dev-server --config webpack/client.dev.js",
        "build": "npm run build:client && npm run build:server",
        "build:server": "cross-env NODE_ENV=production webpack --config webpack/server.prod.js --progress",
        "build:client": "cross-env NODE_ENV=production webpack --config webpack/client.prod.js --progress",
        "start": "cd server && node bin/server.js",
        "lint": "eslint --ext .jsx,.js ./"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vikarshverma/demo.git"
    },
    "keywords": [
        "react",
        "express",
        "webpack",
        "babel",
        "expressjs",
        "react router v4",
        "redux"
    ],
    "author": "Tahnik Mustasin",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vikarshverma/demo.git/issues"
    },
    "homepage": "https://github.com/vikarshverma/demo.git#readme",
    "dependencies": {
        "cross-env": "^5.0.5",
        "ejs": "^2.5.7",
        "express": "^4.14.0",
        "prop-types": "^15.5.10",
        "react": "^15.2.1",
        "react-dom": "^15.2.1",
        "react-hot-loader": "^3.0.0-beta.7",
        "react-redux": "^5.0.3",
        "react-router": "^4.1.2",
        "react-router-dom": "^4.1.2",
        "redux": "^3.5.2"
    },
    "devDependencies": {
        "babel-core": "^6.10.4",
        "babel-loader": "^7.1.1",
        "babel-preset-es2015": "^6.9.0",
        "babel-preset-react": "^6.11.1",
        "babel-preset-stage-1": "^6.5.0",
        "css-loader": "^0.28.7",
        "eslint": "^4.4.1",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-react": "^7.3.0",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^2.30.1",
        "node-sass": "^4.5.3",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.18.2",
        "uglifyjs-webpack-plugin": "^0.4.6",
        "webpack": "^3.5.3",
        "webpack-dev-server": "^2.4.1",
        "webpack-node-externals": "^1.2.0"
    },
    "bundledDependencies": [
        "cross-env",
        "ejs",
        "express",
        "prop-types",
        "react",
        "react-dom",
        "react-redux",
        "react-router",
        "react-router-dom",
        "redux"
    ]
}